{
  "name": "acme/tools-manager",
  "description": "A Laravel Nova resource tool.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0"
  },
  "autoload": {
    "psr-4": {
      "Acme\\ToolsManager\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Acme\\ToolsManager\\ToolServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
